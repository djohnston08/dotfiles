#!/bin/sh

# Source common paths
source ~/projects/dotfiles/common/shell/.shellpaths.common

# Source platform-specific paths
if [[ "$(uname -s)" == "Darwin" ]]; then
    source ~/projects/dotfiles/macos/shell/.shellpaths.macos
elif [[ "$(uname -s)" == "Linux" ]]; then
    source ~/projects/dotfiles/linux/shell/.shellpaths.linux
fi